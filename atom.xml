---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>Blog of Eric Bi</title>
 <link href="http://sxkachilles.com/atom.xml" rel="self"/>
 <link href="http://sxkachilles.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://sxkachilles.com/</id>
 <author>
   <name>Eric Bi</name>
 </author>

 {% for post in site.posts limit:20 %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://sxkachilles.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://sxkachilles.com{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>
